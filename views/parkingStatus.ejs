        <thead>
            <tr>
                <th>Numero slot</th>
                <th>Status</th>
                <th>Posto di ricarica?</th>
            </tr>
        </thead>
        <tbody>
            <% parkings.forEach(function(parking) { %>
                <tr>
                    <td><%= parking.idPosto %></td>
                    <td><%= parking.isOccupato === 1 ? "Occupato" : "Libero" %></td>
                    <td><%= parking.isOnCharge === 1 ? "SÃ¬" : "No" %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</div>
